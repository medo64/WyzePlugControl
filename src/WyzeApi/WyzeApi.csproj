<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>Wyze</AssemblyName>
    <RootNamespace>WyzeApi</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <Product>Wyze</Product>
    <Description>Basic Wyze API.</Description>
  </PropertyGroup>

  <PropertyGroup>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
